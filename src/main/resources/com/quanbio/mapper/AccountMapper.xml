<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quanbio.mapper.AccountMapper">

    <insert id="addAccount">
        insert into user values(#{id},#{userName},#{email},#{birthday},#{familyname},#{firstname},#{gender},#{password},#{middleName},#{roleid})
    </insert>

    <delete id="delete">
        delete from user where id=#{id}
    </delete>
</mapper>
