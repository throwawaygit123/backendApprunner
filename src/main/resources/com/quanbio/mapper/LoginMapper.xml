<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quanbio.mapper.LoginMapper">
    <select id="getByEmailAndPassword" resultType="com.quanbio.model.UserLogin">
        select id,email,password from userlogin where email=#{email} and password=#{password}
    </select>
</mapper>
